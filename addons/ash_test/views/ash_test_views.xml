<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="view_models_form" model="ir.ui.view">
            <field name="name">models.form</field>
            <field name="model">product.template</field>
            <field name="inherit_id" ref="product.product_template_form_view"/>
            <field name="arch" type="xml">
                <data>
                    <xpath expr="//page[@name='general_information']/group" position="after">
                        <group>
                            <field name="tenant_id"/>
                            <field name="sku_code"/>
                            <field name="description"/>
                            <field name="ean"/>
                            <field name="brand"/>
                            <field name="season_code"/>
                            <field name="color"/>
                            <field name="size"/>
                            <field name="merchandise_category_code"/>
                            <field name="style"/>
                            <field name="style_code"/>
                            <field name="weight"/>
                            <field name="returnable"/>
                        </group>
                    </xpath>
                </data>
            </field>
        </record>
    </data>
</odoo>
